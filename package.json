{
  "name": "react-template",
  "version": "1.0.37",
  "description": "A template for creating a new ReactJS project",
  "author": "David LeBlanc <github@david-leblanc.com>",
  "license": "MIT",
  "repository": "git@github.com:d-leb/react-template",
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "analyze": "node scripts/analyze.mjs",
    "build": "node scripts/build.mjs",
    "eslint": "eslint --ext ts,tsx src --max-warnings=0",
    "eslint:fix": "eslint --ext ts,tsx src --fix",
    "lint": "yarn eslint && yarn stylelint",
    "jest": "jest",
    "review": "yarn lint && yarn test && yarn build",
    "prettier": "prettier --check 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "start": "node scripts/start.mjs",
    "stylelint": "stylelint \"src/**/*.ts?\"",
    "stylelint:fix": "stylelint \"src/**/*.ts?\" --fix",
    "test": "yarn tsc",
    "tsc": "tsc"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@axe-core/react": "^4.4.3",
    "@tanstack/react-query": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-intl": "^6.0.8",
    "react-is": "^18.2.0",
    "react-responsive": "^9.0.0-beta.10",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@swc/core": "^1.2.242",
    "@swc/helpers": "^0.4.9",
    "@swc/jest": "^0.2.22",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.8",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.7",
    "@types/react": "^18.0.16",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@yarnpkg/sdks": "^3.0.0-rc.14",
    "browserlist": "^1.0.1",
    "compression-webpack-plugin": "^10.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "imports-loader": "^4.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "mocha": "^10.0.0",
    "ora": "^6.1.2",
    "postcss": "^8.4.16",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "stylelint": "^14.11.0",
    "stylelint-config-concentric-order": "^5.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "svg-url-loader": "^7.1.1",
    "swc-loader": "^0.2.3",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0"
  }
}
