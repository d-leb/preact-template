{
  "name": "react-template",
  "version": "0.8.23",
  "description": "A template for creating a new ReactJS project",
  "author": "David LeBlanc <github@david-leblanc.com>",
  "license": "MIT",
  "repository": "git@github.com:d-leb/react-template",
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "analyze": "node scripts/analyze.mjs",
    "build": "node scripts/build.mjs",
    "eslint": "eslint --ext ts,tsx src --max-warnings=0",
    "eslint:fix": "eslint --ext ts,tsx src --fix",
    "lint": "yarn eslint",
    "review": "yarn lint && yarn test && yarn build",
    "prettier": "prettier --check 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "start": "node scripts/start.mjs",
    "test": "yarn tsc",
    "tsc": "tsc"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@swc/core": "^1.2.218",
    "@types/node": "^16.11.7",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@yarnpkg/sdks": "^3.0.0-rc.14",
    "browserlist": "^1.0.1",
    "compression-webpack-plugin": "^10.0.0",
    "dotenv-webpack": "^8.0.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "imports-loader": "^4.0.0",
    "jest": "^28.1.3",
    "ora": "^6.1.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "svg-url-loader": "^7.1.1",
    "swc-loader": "^0.2.3",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
