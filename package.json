{
  "name": "preact-template",
  "version": "0.8.11",
  "description": "A template for creating a new preact project",
  "author": "David LeBlanc <github@david-leblanc.com>",
  "license": "MIT",
  "repository": "git@github.com:d-leb/preact-template",
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "build": "node scripts/build.mjs",
    "eslint": "eslint --ext ts,tsx src --max-warnings=0",
    "eslint:fix": "eslint --ext ts,tsx src --fix",
    "lint": "yarn eslint",
    "review": "yarn lint && yarn test && yarn build",
    "prettier": "prettier --check 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "start": "node scripts/start.mjs",
    "test": "yarn tsc",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@swc/core": "^1.2.218",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@yarnpkg/sdks": "^3.0.0-rc.12",
    "compression-webpack-plugin": "^10.0.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "ora": "^6.1.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "swc-loader": "^0.2.3",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
