{
  "name": "react-template",
  "version": "1.0.193",
  "description": "A template for creating a new ReactJS project",
  "author": "David LeBlanc <github@david-leblanc.com>",
  "license": "MIT",
  "repository": "git@github.com:d-leb/react-template",
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "analyze": "swc-webpack analyze",
    "build": "swc-webpack build",
    "defaults": "swc-webpack defaults",
    "lint": "yarn prettier",
    "prettier": "prettier --check 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "review": "yarn lint && yarn build",
    "start": "swc-webpack start"
  },
  "dependencies": {
    "@axe-core/react": "^4.5.2",
    "@tanstack/react-query": "^4.20.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-intl": "^6.2.5",
    "react-is": "^18.2.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.6.1",
    "styled-components": "^5.3.6",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@d-leb/swc-webpack": "^0.1.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  }
}
